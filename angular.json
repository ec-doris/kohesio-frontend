{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "defaultProject": "app",
    "projects": {
      "app": {
        "root": "",
        "sourceRoot": "src",
        "projectType": "application",
        "prefix": "app",
        "schematics": {
          "@schematics/angular:component": {
            "styleext": "scss"
          }
        },
        "architect": {
          "build": {
            "builder": "ngx-build-plus:build",
            "options": {
              "outputPath": "dist",
              "index": "src/index.html",
              "main": "src/main.ts",
              "polyfills": "src/polyfills.ts",
              "tsConfig": "src/tsconfig.app.json",
              "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "glob": "**/*",
                  "input": "node_modules/@eui/core/assets/",
                  "output": "./assets"
                },
                {
                  "glob": "**/*",
                  "input": "node_modules/@eui/styles/dist/assets/images",
                  "output": "./assets/images"
                }
              ],
              "styles": [
                "node_modules/@eui/styles/dist/assets/fonts/webfont/Roboto/Roboto.css",
                "node_modules/@eui/styles/dist/styles/eui-primeng.css",
                "node_modules/@eui/styles/dist/styles/eui.css",
                  "node_modules/@eui/styles/dist/assets/fonts/font-awesome/font-awesome.min.css",
                  "node_modules/@eui/styles/dist/assets/fonts/flag-icons/css/flag-icon.min.css",
                "src/styles.scss"
              ]
            },
            "configurations": {
              "production": {
                "fileReplacements": [
                  {
                    "replace": "src/environments/environment.ts",
                    "with": "src/environments/environment.prod.ts"
                  }
                ],
                "optimization": true,
                "outputHashing": "all",
                "sourceMap": false,
                "extractCss": true,
                "namedChunks": true,
                "aot": true,
                "extractLicenses": true,
                "vendorChunk": false,
                "buildOptimizer": true,
                "budgets": [
                  {
                    "type": "initial",
                    "maximumWarning": "3mb",
                    "maximumError": "5mb"
                  }
                ],
                "proxyConfig": "proxy.conf.json"
              },
              "dev": {
                "fileReplacements": [
                  {
                    "replace": "src/environments/environment.ts",
                    "with": "src/environments/environment.dev.ts"
                  }
                ],
                  "proxyConfig": "proxy.conf.json"
              }
            }
          },
          "serve": {
            "builder": "@angular-devkit/build-angular:dev-server",
            "options": {
              "browserTarget": "app:build"
            },
            "configurations": {
              "proxy-mock": {
                "browserTarget": "app:build",
                "proxyConfig": "proxy-mock.conf.json"
              },
              "proxy": {
                "browserTarget": "app:build",
                "proxyConfig": "proxy.conf.json"
              },
              "production": {
                "browserTarget": "app:build:production"
              }
            }
          },
          "test": {
            "builder": "@angular-devkit/build-angular:karma",
            "options": {
              "main": "src/test.ts",
              "polyfills": "src/polyfills.ts",
              "tsConfig": "src/tsconfig.spec.json",
              "karmaConfig": "src/karma.conf.js",
              "styles": [
                "src/styles.scss"
              ],
              "scripts": [],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                {
                  "glob": "**/*",
                  "input": "node_modules/@eui/core/assets/",
                  "output": "./assets"
                }
              ]
            }
          },
          "lint": {
            "builder": "@angular-devkit/build-angular:tslint",
            "options": {
              "tsConfig": [
                "src/tsconfig.app.json",
                "src/tsconfig.spec.json"
              ],
              "exclude": [
                "**/node_modules/**"
              ]
            }
          }
        }
      },
      "app-e2e": {
        "root": "e2e/",
        "projectType": "application",
        "prefix": "",
        "architect": {
          "e2e": {
            "builder": "@angular-devkit/build-angular:protractor",
            "options": {
              "protractorConfig": "e2e/protractor.conf.js",
              "devServerTarget": "app:serve"
            },
            "configurations": {
              "production": {
                "devServerTarget": "app:serve:production"
              }
            }
          },
          "lint": {
            "builder": "@angular-devkit/build-angular:tslint",
            "options": {
              "tsConfig": "e2e/tsconfig.e2e.json",
              "exclude": [
                "**/node_modules/**"
              ]
            }
          }
        }
      }
    }
  }
